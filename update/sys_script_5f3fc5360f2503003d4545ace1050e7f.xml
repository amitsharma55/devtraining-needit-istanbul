        <script><![CDATA[function getOPISApprovals() {
	var u = gs.getUserID();
	var answer = new Array();
	var i = 0;
	answer[i++] = new String(u);
	var g = new GlideRecord("OPIS");
	g.addQuery("u_manager", u);
	//g.addQuery("approvals", "true");
	//g.addQuery("starts", "<=", gs.daysAgo(0));
	//g.addQuery("ends", ">=", gs.daysAgo(0));
	g.query();
	while( g.next())
		answer[i++] = new String(g.user);
	
	return answer;
}]]></script>
